{
  "name": "fincorpx",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:testing": "vite --mode testing",
    "dev:demo": "vite --mode demo",
    "dev:production": "vite --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:development": "run-p type-check \"build-only:development {@}\" --",
    "build:testing": "run-p type-check \"build-only:testing {@}\" --",
    "build:demo": "run-p type-check \"build-only:demo {@}\" --",
    "build:production": "run-p type-check \"build-only:production {@}\" --",
    "build:all": "npm run build:development && npm run build:testing && npm run build:demo && npm run build:production",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:development": "vite build --mode development --outDir dist-development",
    "build-only:testing": "vite build --mode testing --outDir dist-testing",
    "build-only:demo": "vite build --mode demo --outDir dist-demo",
    "build-only:production": "vite build --mode production --outDir dist-production",
    "type-check": "vue-tsc --build",
    "docker:build:all": "docker build --build-arg ENV=development -t fincorpx:dev . && docker build --build-arg ENV=testing -t fincorpx:test . && docker build --build-arg ENV=demo -t fincorpx:demo . && docker build --build-arg ENV=production -t fincorpx:prod ."
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@primeuix/themes": "^1.2.1",
    "axios": "^1.10.0",
    "firebase": "^12.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.17",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "typescript": "~5.8.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  }
}
